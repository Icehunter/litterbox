{
  "name": "@icehunter/litterbox-redis",
  "version": "4.1.1",
  "description": "> TODO: description",
  "author": "Ryan Wilson <syndicated.life@gmail.com>",
  "homepage": "https://github.com/Icehunter/litterbox#readme",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "types": "types",
    "lib": "lib",
    "src": "src"
  },
  "files": [
    "types",
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Icehunter/litterbox.git"
  },
  "scripts": {
    "test": "echo \"Error: run tests from root\" && exit 1",
    "link": "yarn link",
    "unlink": "yarn unlink",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "perf": "yarn build && node performanceTest.js",
    "build": "rimraf lib && yarn build:ts && yarn build:types",
    "build:ts": "tsc",
    "build:types": "dts-bundle --name @icehunter/litterbox-redis --main types/index.d.ts --out ../lib/index.d.ts",
    "update": "npx npm-check -u"
  },
  "bugs": {
    "url": "https://github.com/Icehunter/litterbox/issues"
  },
  "dependencies": {
    "@hapi/hoek": "^9.2.0",
    "@icehunter/litterbox": "^4.1.1",
    "bluebird": "^3.5.3",
    "redis": "^3.1.2"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.34",
    "@types/hoek": "^4.1.3",
    "@types/node": "^15.0.2",
    "@types/redis": "^2.8.28",
    "dts-bundle": "^0.7.3",
    "rimraf": "3.0.2",
    "typescript": "^4.2.4"
  },
  "lint-staged": {
    "src/**/*.{ts,json}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
