{
  "name": "@icehunter/litterbox-redis",
  "version": "4.1.2",
  "description": "> TODO: description",
  "author": "Ryan Wilson <syndicated.life@gmail.com>",
  "homepage": "https://github.com/Icehunter/litterbox#readme",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "types": "types",
    "lib": "lib",
    "src": "src"
  },
  "files": [
    "types",
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Icehunter/litterbox.git"
  },
  "scripts": {
    "test": "echo \"Error: run tests from root\" && exit 1",
    "link": "yarn link",
    "unlink": "yarn unlink",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "perf": "yarn build && node performanceTest.js",
    "build": "rimraf lib && yarn build:ts && yarn build:types",
    "build:ts": "tsc",
    "build:types": "dts-bundle --name @icehunter/litterbox-redis --main types/index.d.ts --out ../lib/index.d.ts",
    "update": "npx npm-check -u"
  },
  "bugs": {
    "url": "https://github.com/Icehunter/litterbox/issues"
  },
  "dependencies": {
    "@hapi/hoek": "11.0.2",
    "@icehunter/litterbox": "4.1.2",
    "bluebird": "3.7.2",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "redis": "4.6.7"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.38",
    "@types/hoek": "4.1.4",
    "@types/node": "20.4.2",
    "@types/redis": "4.0.11",
    "dts-bundle": "0.7.3",
    "rimraf": "5.0.1",
    "typescript": "5.1.6"
  },
  "lint-staged": {
    "src/**/*.{ts,json}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
