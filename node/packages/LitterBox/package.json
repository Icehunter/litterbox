{
  "name": "@icehunter/litterbox",
  "version": "1.2.1",
  "description": "> TODO: description",
  "author": "Ryan Wilson <syndicated.life@gmail.com>",
  "homepage": "https://github.com/Icehunter/litterbox#readme",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "src/index.js",
  "directories": {
    "lib": "lib",
    "src": "src"
  },
  "files": [
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Icehunter/litterbox.git"
  },
  "scripts": {
    "test": "echo \"Error: run tests from root\" && exit 1",
    "build": "rm -rf lib && babel src -s -d lib --copy-files --ignore __tests__,spec.js,test.js,__snapshots__",
    "updater": "npm-check -u"
  },
  "bugs": {
    "url": "https://github.com/Icehunter/litterbox/issues"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "7.1.6",
    "@babel/register": "^7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-loader": "8.0.4",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-named-asset-import": "^0.3.0",
    "babel-preset-react-app": "^7.0.0",
    "eslint": "5.6.0",
    "eslint-config-react-app": "^3.0.6",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "7.11.1",
    "flow": "^0.2.3",
    "flow-bin": "^0.92.0",
    "flow-typed": "^2.5.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.1",
    "npm-check": "^5.9.0",
    "prettier": "^1.16.3"
  },
  "lint-staged": {
    "src/**/*.{js,json}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "src/**/*.{css}": [
      "prettier --write",
      "git add"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
